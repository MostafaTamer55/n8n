    {
      "parameters": {
        "language": "pythonNative",
        "pythonCode": "# Language: Python | Mode: Run Once for All Items\nresults = []\n\nfor item in _items:\n    data = item.get('json', {})\n    # Find text in data['result']['text'] (Common n8n LLM path)\n    text = data.get('text') or data.get('result', {}).get('text')\n    \n    if not text or \"Items to Prepare:\" not in text:\n        continue\n\n    # Extract the items section\n    items_section = text.split(\"Items to Prepare:\")[1].split(\"Total Amount:\")[0]\n    lines = items_section.strip().split('\\n')\n    \n    for line in lines:\n        # Clean line: remove bullet points and emojis\n        clean = line.strip().lstrip('‚Ä¢').lstrip('*').strip()\n        \n        if clean:\n            # 1. Extract Quantity (digits at the end or after a colon)\n            qty_digits = \"\".join(filter(str.isdigit, clean.split(':')[-1])) if ':' in clean else \"\".join(filter(str.isdigit, clean))\n            quantity = int(qty_digits) if qty_digits else 1\n            \n            # 2. Extract Base Name (Remove options in parentheses and quantities)\n            # Example: \"Gourmet Burger (Pickles): 2\" -> \"Gourmet Burger\"\n            base_name = clean.split('(')[0].split(':')[0].strip()\n            \n            results.append({\n                \"json\": {\n                    \"Item_Name\": base_name, # Search Google Sheets for THIS\n                    \"Full_Description\": clean, # Use THIS for the Telegram message\n                    \"Item_Quantity\": quantity,\n                }\n            })\n\nreturn results"
      },
